define("interface-widget-1:fileRecoveryDialog/start.js",function(e,i,t){var o=e("base:widget/libs/jquerypacket.js"),a=e("base:widget/storage/storage.js"),s="file_recovery_dialog_"+window.yunData.MYUK,d='<div class="file-recovery-dialog-body"><div class="icon"></div><div>您的帐号疑似被盗</div><div>您的文件可能已被恶意删除，我们将尽量在一周内为您找回近期被删文件</div><div class="recovery-tips">找回文件地址：<i class="file-icon"></i><span> [重要恢复] 您在16年07月01号之后被删除的文件</span></div></div>';t.exports.start=function(e){a.getItem(s)||o.ajax({url:"/api/checkrecover",method:"get",dataType:"json",success:function(i){if(0===+i.errno&&2==+i.show){var t=e.ui.window({title:"提示",body:'<div style="text-align:center;padding:22px;">'+d+"</div>",width:"540px",className:"file-recovery-dialog",noFooter:!1,show:!0,buttons:[{name:"confirm",title:"我知道了",color:"blue-large",click:function(){t.hide()}}]});a.setItem(s,1)}}})}});